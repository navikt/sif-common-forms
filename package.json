{
    "name": "@navikt/sif-common-forms",
    "version": "3.4.1",
    "private": false,
    "scripts": {
        "dev": "craco start",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
        "eject": "react-scripts eject",
        "build-lib": "npm-run-all clean-lib compile-lib-script copy-files-to-lib",
        "clean-lib": "rimraf ./lib",
        "compile-lib-script": "tsc -p tsconfig-build-lib.json",
        "copy-files-to-lib": "copyfiles -u 2 \"src/forms/**/*.less\" lib/ && copyfiles -u 2 \"src/forms/**/assets/**/*\" lib/ && copyfiles -u 2 \"src/forms/**/*.json\" lib/",
        "build-dev-app": "craco build",
        "predeploy": "npm run build-dev-app",
        "deploy": "gh-pages -d build",
        "patchit": "npm-run-all build-lib patch do-publish",
        "patch": "npm version patch",
        "do-publish": "npm publish"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "dependencies": {},
    "peerDependencies": {
        "@navikt/sif-common-core": "^3.1.1",
        "@navikt/sif-common-formik": "^5.0.1"
    },
    "devDependencies": {
        "@craco/craco": "^5.6.3",
        "@formatjs/intl-pluralrules": "^1.5.2",
        "@navikt/sif-common-core": "^2.3.0",
        "@navikt/sif-common-formik": "^5.0.0",
        "@testing-library/jest-dom": "^5.1.0",
        "@testing-library/react": "^9.4.0",
        "@testing-library/user-event": "^8.1.0",
        "@types/jest": "^25.1.1",
        "@types/node": "^13.7.0",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-responsive": "^8.0.2",
        "axios": "^0.19.2",
        "copyfiles": "^2.2.0",
        "core-js": "^3.6.4",
        "craco-less": "^1.15.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "formik": "^2.1.3",
        "gh-pages": "^2.2.0",
        "history": "^4.10.1",
        "intl": "^1.2.5",
        "jest-css-modules": "^2.1.0",
        "less-plugin-npm-import": "^2.1.0",
        "moment": "^2.24.0",
        "npm-run-all": "^4.1.5",
        "react": "^16.12.0",
        "react-aria-menubutton": "^6.3.0",
        "react-collapse": "^5.0.1",
        "react-day-picker": "^7.4.0",
        "react-dom": "^16.12.0",
        "react-intl": "^3.11.0",
        "react-modal": "^3.11.1",
        "react-responsive": "^8.0.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^25.2.0",
        "tslint": "^6.0.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^4.2.0",
        "typescript": "^3.7.5"
    },
    "description": "Fellesskjema for sykdom i familien",
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/sif-common-forms.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/navikt/sif-common-forms/issues"
    },
    "homepage": "https://navikt.github.io/sif-common-forms",
    "main": "lib/index.js",
    "types": "lib/index.d.js"
}
